# Pruebas Unitarias para la Matriz Dispersa

require "matrices.rb"
require "test/unit"


class Test_MatrizDispersa < Test::Unit::TestCase
       
        def test_1
        
                assert_equal MatrizDispersa.new(3,3,[0,2,4,0,2,4,2,4,6]), (MatrizDispersa.new(3,3,[0,1,2,0,1,2,1,2,3]) + MatrizDispersa.new(3,3,[0,1,2,0,1,2,1,2,3]))        
                
        end

end
